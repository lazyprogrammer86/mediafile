<%-include("partials/header")%>
<body onload="hideButtons();">
</body>
<div class="row uplo">
<form class="card col-md-6" action="/upload" method="post" enctype="multipart/form-data">
<h1>Upload Here!</h1>
<p id="regwarn" style="color:red"><%=uploWarning%></p>
<input class="form-control" type="text" name="name" placeholder="Name" required>
<textarea class="form-control" name="desc" rows="7" placeholder="Description" cols="80"></textarea>
<input class="form-control" type="file" name="fileName" placeholder="select a photo or video file" required>
<button class="btn btn-lg btn-primary" type="submit" name="button" >Upload</button>
</form>
</div>
<%-include("partials/footer")%>
