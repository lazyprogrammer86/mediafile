<%-include("partials/header")-%>
<body onload="hideLogButton()">
</body>
<div class="row log">
<form class="card col-md-6" action="/login" method="post">
<h1>Sign In</h1>
<p id="logwarn" style="color:red"><%=logWarning%></p>
<input class="form-control" type="text" name="username" value="" placeholder="Email Id" required>
<input class="form-control" type="password" name="password" value="" placeholder="Password" required>
<a href="/everify">forgot password?</a>
<button class="btn btn-lg btn-outline-success" type="submit" name="button">Sign In</button>
</form>
</div>
<%-include("partials/footer")-%>
